void dump_int_array(int[] array) {
    int i;
    print("{");
    for (i = 0; i < array.size(); i++) {
	if (i > 0) {
	    print(", ");
	}
	print("" + array[i]);
    }
    print("}\n");
}

int_array.resize(10);
dump_int_array(int_array);
int_array.resize(2);
dump_int_array(int_array);

int_array.insert(0, 3);
dump_int_array(int_array);
int_array.insert(3, -5);
