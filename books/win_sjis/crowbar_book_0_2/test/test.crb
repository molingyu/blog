function hoge(a, b) {
    c = a + b;
    print("a+b.." + (a + b) + "\n");

    return c;
}

function int_func() {
    print("int func in\n");
    for (i = 0; i < 10; i = i + 1) {
	print("i.." + i + "\n");
	return 1;
    }
    print("int func out\n");
}

function real_func() {
    print("real func in\n");
    for (i = 0; i < 10; i = i + 1) {
	print("i.." + i + "\n");
	return 0.1;
    }
    print("real func out\n");
}

function string_func() {
    zzz = "zzz";
